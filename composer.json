{
    "name": "thopra/styleguide",
    "description": "Styleguide skeleton based on kss-php, containing documentation of bootstrap components",
    "keywords": [
        "kss",
        "styleguide",
        "css documentation"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/finder": "~2.1",
        "kss-php/kss-php": "2.0.*",
        "namelesscoder/fluid": "*",
        "zendframework/zend-cache": "2.5.*",
        "zendframework/zend-serializer": "2.5.*",
        "endroid/qrcode": "^1.5"
    },
    "authors": [
        {
            "name": "Thomas Prangenberg",
            "email": "t@thomas-prangenberg.de",
            "homepage": "http://www.thomas-prangenberg.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Thopra\\": "Classes/"
        }
    }
}
